<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/manageCoursesFragment">

    <fragment
        android:id="@+id/approveStaffFragment"
        android:name="com.saa.staff.activities.mainui.ApproveStaff.ApproveStaffFragment"
        android:label="approve_staff_fragment"
        tools:layout="@layout/approve_staff_fragment" />
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.saa.staff.activities.mainui.EditProfile.EditProfileFragment"
        android:label="edit_profile_fragment"
        tools:layout="@layout/edit_profile_fragment" />
    <fragment
        android:id="@+id/manageCoursesFragment"
        android:name="com.saa.staff.activities.mainui.ManageCourses.ManageCoursesFragment"
        android:label="manage_courses_fragment"
        tools:layout="@layout/manage_courses_fragment" >
        <action
            android:id="@+id/action_manageCoursesFragment_to_addEditCourseFragment"
            app:destination="@id/addEditCourseFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_manageCoursesFragment_to_viewCourseInfoFragment"
            app:destination="@id/viewCourseInfoFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/manageFellowshipFragment"
        android:name="com.saa.staff.activities.mainui.ManageFellowship.ManageFellowshipFragment"
        android:label="manage_fellowship_fragment"
        tools:layout="@layout/manage_fellowship_fragment" >
        <action
            android:id="@+id/action_manageFellowshipFragment_to_viewFellowshipFragment"
            app:destination="@id/viewFellowshipFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_manageFellowshipFragment_to_addEditFellowshipFragment"
            app:destination="@id/addEditFellowshipFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/manageScholarshipFragment"
        android:name="com.saa.staff.activities.mainui.ManageScholarship.ManageScholarshipFragment"
        android:label="manage_scholarship_fragment"
        tools:layout="@layout/manage_scholarship_fragment" />
    <fragment
        android:id="@+id/reviewApplicationFragment"
        android:name="com.saa.staff.activities.mainui.ReviewApplication.ReviewApplicationFragment"
        android:label="review_application_fragment"
        tools:layout="@layout/review_application_fragment" />
    <fragment
        android:id="@+id/sendNotificationFragment"
        android:name="com.saa.staff.activities.mainui.SendNotification.SendNotificationFragment"
        android:label="send_notification_fragment"
        tools:layout="@layout/send_notification_fragment" />
    <fragment
        android:id="@+id/trainingProgressFragment"
        android:name="com.saa.staff.activities.mainui.TrainingProgress.TrainingProgressFragment"
        android:label="training_progress_fragment"
        tools:layout="@layout/training_progress_fragment" />
    <fragment
        android:id="@+id/manageDiplomaFragment"
        android:name="com.saa.staff.activities.mainui.ManageDiploma.ManageDiplomaFragment"
        android:label="manage_diploma_fragment"
        tools:layout="@layout/manage_diploma_fragment" />
    <fragment
        android:id="@+id/addEditCourseFragment"
        android:name="com.saa.staff.activities.mainui.ManageCourses.AddEditCourseFragment"
        android:label="AddEditCourseFragment" />
    <fragment
        android:id="@+id/viewCourseInfoFragment"
        android:name="com.saa.staff.activities.mainui.ManageCourses.ViewCourseInfoFragment"
        android:label="ViewCourseInfoFragment" />
    <fragment
        android:id="@+id/viewFellowshipFragment"
        android:name="com.saa.staff.activities.mainui.ManageFellowship.ViewFellowshipFragment"
        android:label="ViewFellowshipFragment" />
    <fragment
        android:id="@+id/addEditFellowshipFragment"
        android:name="com.saa.staff.activities.mainui.ManageFellowship.AddEditFellowshipFragment"
        android:label="add_edit_fellowship_fragment"
        tools:layout="@layout/add_edit_fellowship_fragment" />
</navigation>